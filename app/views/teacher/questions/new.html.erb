<div class="page-header">
  <%= link_to teacher_questions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <h1>New Question</h1>
  <hr class="colorgraph">
</div>

<%= form_for(@question, {url: teacher_questions_path, html: { class: "form-horizontal", role: "form" }}) do |field| %>
